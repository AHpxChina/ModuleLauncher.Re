        <OutputType>Library</OutputType>
        <PackageId>ModuleLauncher.Re</PackageId>
        <Version>2.7.0</Version>
        <Authors>AHpx</Authors>
        <Company>AHpx</Company>
        <Product>AHpx.ModuleLauncher</Product>
